        <api_name>x_58872_needit.addEncodedQueryDemo</api_name>
        <caller_access/>
        <client_callable>false</client_callable>
        <description>This on-demand script include will demonstrate the addEncodedQuery() method of the GlideRecord Server side API.  Retuens number of recordsas per query condition.</description>
        <name>addEncodedQueryDemo</name>
        <script><![CDATA[function addEncodedQueryDemo()
{
	var recObj = new GlideRecord('needit');
	recObj.addEncodedQuery('assigned_toSTARTSWITHA^ORstate=13^ORpriority=4');
	recObj.query();
	var c=0;
	while(recObj.next())
		{
			c+=1;
		}
	return c;
}]]></script>
